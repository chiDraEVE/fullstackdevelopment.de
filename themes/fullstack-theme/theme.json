					"gradient": "linear-gradient(180deg, var(--wp--preset--color--primary) 0%, var(--wp--preset--color--primary-dark) 100%)",
					"name": "Primary Overlay",
					"slug": "primary-overlay"
				},
				{
					"gradient": "linear-gradient(180deg, rgba(18,21,23,0.0) 0%, var(--wp--preset--color--dark) 90%)",
					"name": "Hero Video Overlay",
					"slug": "hero-video"
				},
				{
					"gradient": "linear-gradient(135deg, var(--wp--preset--color--secondary-light), var(--wp--preset--color--secondary))",
					"name": "Secondary Soft",
					"slug": "secondary-soft"
				}
			],
